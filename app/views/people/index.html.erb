<h1>Listing of Personnel</h1>
 
<table class = "table">
  <tr>
    <th>Name: </th>
    <th>Role: </th>
  </tr>
 
  <% @people.each do |person| %>
    <tr>
      <td><a href=<%= person_path(person)  %>> <%= person.name %> </a> </td>
      <td><%= person.group.capitalize %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'Back', root_url %>
