
<center>
<h1>Admin Login</h1>
</center>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:admin, url: admins_login_path) do |f| %>

      <%= f.label :username %>
      <%= f.text_field :username, class: 'form-control', value: 'admin'%>

      <%= f.label :password %>
      <%= f.password_field :password, class: 'form-control' %>
      </br>
      <%= f.submit "Sign in", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
